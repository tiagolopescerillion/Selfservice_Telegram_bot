spring:
  security:
    oauth2:
      client:
        registration:
          rhsso:
            client-id: ${RHSSO_CLIENT_ID}
            client-secret: ${RHSSO_CLIENT_SECRET}
            authorization-grant-type: authorization_code
            redirect-uri: "{baseUrl}/login/oauth2/code/rhsso"
        provider:
          rhsso:
            issuer-uri: ${RHSSO_ISSUER_URI}
